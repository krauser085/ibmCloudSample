    <!DOCTYPE html>
    <html>
    <head>
        <meta charset="utf-8" />
    </head>
    <body>
        <% for(var i=0; i<users.length ;i++){ %>
            <div class="info">
                <h2 class="name"><%= users[i].name %></h2>   
                <%= users[i].age %>   
                <%= users[i].address %>
                <%= users[i]._id %> <br><br>
                <%= users[i] %> 
                <input type="button" class="deleteBtn" value="delete"
                data-id="<%= users[i]._id %>" data-rev="<%= users[i]._rev %>" >
            </div>
       <% } %>
    
    <script type="text/javascript" src="/javascripts/showUser.js"></script>
    </body>
    </html>